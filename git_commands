/*"Hello, this is a list of the most popular commands for Git"*/
/*"Привет, это список самых популярных команд для Git"*/

git --version //# Очевидно версия

https://www.git-scm.com/ //# Ссылка

git config --global user.name "Имя пользователя"

git config --global user.email Почта

git config --list //# НАстройки пойльзователя


cd ИМЯ_ПАПКИ //#Переход в дргую папку

mkdir ИМЯПАПКИ //# Переход

echo "Some text" > file.txt //# Файл с текстом принтует

ls //# Что в папке

cat file.txt//# Чтение файла
rm file.txt//# Удаление файла

pwd - //# полный путь
git status //# Состояние индекса гита, показывает те измениения подготовленные для коммита

git add FILENAME //# Добавить файл в индекс гита

git add . //# Доюавить све файлы в индекс гита

git commit //# Сохраненине текущих изменений + комментарий

git commit -m "Комментарий к комиту"
//# commit - указатель на дерево  коревой папки
//# указатель HEAD ссылается на ту версию проекта которая находится в рабочей директории \ ссылка на ветку

git checkout //# Перейти к определенной версии /master/main
//# detached HEAD работать надо в состоянии веток, комиты делать в ветках


git branch "Имя ветки" //# Имя ветки создается в связи с разрабатываемый функционал

git checkout -b "Имя ветки" //# Созданрие и переход в новую ветку

git branch //# список всех веток
git branch -m "new_name_ветки" //# Переименование текущей ветки

git branch -d "Имя ветки" //# Удаляет ветку, не текущую
git merge "Имя_ветки" //# Для слияния с текущей веткой feature branch/receiving branch
git merge -m "КОМЕНТ СЛИЯНИЯ С ПЕНАВЕТКОЙ" Pena_vetka //#


code . //#Запускает код из папки на маке надо ток инсталл Shell PATH чтоб был
/* Подключение и закачка гит*/
git remote //# Проверка на удаленные сервера
git remote add origin https://github.com/HageGyakutai/programming_practice.githttps://github.com/HageGyakutai/programming_practice.git //# подключение удаленного сервера

git clone <сайт> //

git branch -M main // Ветка 
git push -u origin main //Скачать ветку

git push -u origin main //# Загрузка на сервер origin

git push //# Отправить
git pull //# Тянуть


